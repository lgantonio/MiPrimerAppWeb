<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>ConfirmarOpServlet</servlet-name>
		<display-name>ConfirmarOpServlet</display-name>
		<description></description>
		<servlet-class>es.open4job.web.ConfirmarOpServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>VisualizarOpServlet</servlet-name>
		<display-name>VisualizarOpServlet</display-name>
		<description></description>
		<servlet-class>es.open4job.web.VisualizarOpServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ConfirmarOpServlet</servlet-name>
		<url-pattern>/ConfirmarOpServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>VisualizarOpServlet</servlet-name>
		<url-pattern>/VisualizarOpServlet</url-pattern>
	</servlet-mapping>

	<resource-ref>
		<description>Oracle Datasource</description>
		<res-ref-name>jdbc/myoracle</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>

</web-app>
